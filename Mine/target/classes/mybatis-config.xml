<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<!-- 마이바티스 설정파일 type에 명시된 클래스를 함축이름으로 사용합니다. -->
<configuration>


	<properties>
		<property name="TABLE_UPLOAD" value="fileupload" />
		<property name="PNO" value="pno" />
		<property name="PCATEGORY" value="pCategory" />
		<property name="PNAME" value="pName" />
		<property name="PTITLE" value="pTitle"/>
		<property name="PBRAND" value="pBrand" />
		<property name="UNIQUENAME" value="uniqueName" />
		<property name="PRICE" value="price" />
		<property name="PDETAIL" value="pDetail" />
		<property name="P_IMGADDR" value="p_imgAddr" />
		<property name="REGDATE" value="regdate" />
	</properties>
	
	<typeAliases>
		<typeAlias type="com.itwill.command.UserVO" alias="UserVO" />
		<typeAlias type="com.itwill.command.FileUploadVO"
			alias="FileUploadVO" />
	</typeAliases>
	
</configuration>
  	